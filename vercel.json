{
    "version":2,
    "builds": [
        {
            "src":"index.js",
            "use":"@vercel/node"
        }
    ],
    "routes": [
        {
            "src":"/controllers/(.*)",
            "dest":"index.js"
        }, 
        {
            "src": "/(.*)",
            "dest": "/index.html"
          }
    ]
}